{
  "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "declaration": false,
      "noImplicitAny": false,
      "removeComments": true,
      "noLib": false,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "sourceMap": true,
      "outDir": "build",
      "typeRoots": ["node_modules/@types"],
      "types": ["node"]
  },
  "exclude": [
      "node_modules"
  ],
  "compileOnSave": false
}
